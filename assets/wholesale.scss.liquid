.hero-button{
    color: rgba(27, 67, 136, 1);
    background: #fff;
    border-radius: 50px;
    font-size: 13px;
    font-family: Gotham;
    display: inline-block;
    padding-top: 16px;
    padding-left: 65px;
    padding-bottom: 16px;
    padding-right: 65px;
    position: absolute;
    bottom: -90px;
    left: 0;
    transition: 0.2s ease-in-out;
    &:hover{
      border-color: rgba(255, 255, 255, 1) !important;
      background-color: rgba(27, 67, 136, 1) !important;
      color: rgba(255, 255, 255, 1) !important;
    }
    @media screen and (max-width: 768px){
      position: unset;
      max-width: unset;
      width: 100%;
      text-align: center;
      margin-top: 32px;
    }
    @media screen and (max-width: 767px){
      padding: 16px 0px;
      margin-top: 40px;
      letter-spacing: 1px;
    }
}
.hero-banner-main{
  @media screen and (max-width: 767px){
    margin-bottom: 30px;
  }
  .hero-banner{
    position: relative;
    width: 100%;
    @media screen and (max-width: 768px){
      display: flex;
      flex-direction: column-reverse;
      flex-wrap: wrap;
    }
    img{
      width: 100%;
      border-top-right-radius: 20px;
      border-top-left-radius: 20px;
      display: block;
      @media screen and (max-width: 768px){
        border-radius: unset;
        position: relative;
        z-index: -1;
        margin-top: -10px;
      }
       @media screen and (max-width: 767px){
        height: 30vh;
        object-fit: cover;
        object-position: right;
       }
    }
    .hero-text-box{
        background: #fff;
        position: absolute;
        top: 47%;
        transform: translateY(-50%);
        border-radius: 12px;
        max-width: 856px;
        padding: 40px;
        left: 45px;
        @media screen and (max-width: 1440px){
          max-width: 545px;
          
        }
       @media screen and (max-width: 1100px){
          max-width: 450px;
          top: 40%;
       }
       @media screen and (max-width: 1024px){
          top: 40%;
       }
      @media screen and (max-width: 768px){
        position: unset;
        max-width: unset;
        transform: unset;
        width: 100%;
        background-color: #edeff1;
        border-bottom-right-radius: unset;
        border-bottom-left-radius: unset;
      }
      @media screen and (max-width: 767px){
        padding-bottom: 30px;
      }
    }
    
  }
 
}
.hero-marquee{
  padding: 0 42px;
}
.marquee-container{
  background: rgba(27, 67, 136, 1);
  border-bottom-right-radius: 20px;
  border-bottom-left-radius: 20px;
  overflow: hidden;
  @media screen and (max-width: 1000px){
    display: none;
  }
}
.marquee {
    display:  inline-block;
    white-space: nowrap;
    position: relative;
    transform: translate3d(0%, 0, 0);
    animation-name: marquee;
    animation-timing-function: linear;
    animation-iteration-count: infinite;
	animation-duration: 1000s;
	color: #fff;
	letter-spacing: 1px;
    padding-top: 28px;
    padding-bottom: 28px;
    line-height: 1.27em;
    font-family: Gotham-book;
    font-size: 15px;
}
.marquee:hover { 
  animation-play-state: paused;
}
.marquee a {
    display: inline-block;
    white-space: nowrap;
    padding-right: 0rem;
    text-transform: uppercase;
    font-family: Gotham-book;
    font-size: 15px;
    letter-spacing: 1px;
    color: #fff;
    line-height: 1.27em;
	text-decoration: none;
}

@keyframes marquee {
    0% {
        transform: translate3d(50vw, 0, 0);
    }

    100% {
        transform: translate3d(-100%, 0, 0);
    }
}

