<div class="logo-list-wrapper" id="sec-logo">
  {% if section.settings.heading != blank %}
  <h2>{{ section.settings.heading }}</h2>
  <p>{{product.title}}</p>
  {% endif %}

{%comment%}
  {% if section.blocks.size > 0 %}
  <div class="logos">
    {% for block in section.blocks %}
    {% if block.settings.image != blank %}
    <a href="{{ block.settings.url }}">
    <img src="{{ block.settings.image | img_url: 'master' }}" alt="{{ block.settings.image.alt }}">
    </a>
    {% endif %}
    {% endfor %}
  </div>
  {% endif %}
  {%endcomment%}
  <div class="logos">
    
  </div>
</div>
{% if product.id == 191293685775 or product.id == 191293718543 or product.id == 191306727439 or product.id == 191306760207 %}

<div class="coupon_label">
  <p>
   <a href="https://shop.hylands.com/">
     {%comment%}<img src="//cdn.shopify.com/s/files/1/1533/0491/t/54/assets/locator.png?v=6905894776310980894" alt="locator">{%endcomment%} {{ section.settings.coupon_label }}
   </a>
  </p>
  <div id="coupon-popup-overlay" class="coupon-popup-overlay"></div>
  <p id="coupon-popup" class="coupon-popup">
    <span class="coupon-head">{{ section.settings.coupon_head }}</span>
    <span class="close-pop"><img src="https://cdn.shopify.com/s/files/1/1533/0491/t/54/assets/close-icon-new.png?v=1637922270"></span>
    <img id="imageresource" src="https://cdn.shopify.com/s/files/1/1533/0491/t/54/assets/Coupon.png?v=1637912045">
    <a class="view-coupon" href="https://cdn.shopify.com/s/files/1/1533/0491/t/54/assets/Coupon.png?v=1637912045"  target="_blank">Save Coupon</a> 
  </p>
</div>
{% endif %}
